{
  "name": "fullstack-dms",
  "version": "1.0.0",
  "description": "A fullstack document management application",
  "main": "index.js",
  "scripts": {
    "start:dev": "./node_modules/.bin/nodemon --watch server --exec babel-node -- server/bin/www",
    "build:dist": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./server/test,./client/test,./screenshots,./reports,./migrations,./coverage,docs --copy-files",
    "start": "npm run build:dist && node dist/server/index.js",
    "test:watch": "NODE_ENV=test istanbul cover _mocha -- server/test/**/*.test.js --compilers js:babel-core/register --timeout 30000 --watch",
    "test": "NODE_ENV=test rimraf dist/ && istanbul cover _mocha -- server/test/**/*.test.js --compilers js:babel-core/register --timeout 30000",
    "client:test": "mocha --reporter progress testSetup.js client/test/**/*.test.js",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-aawa/fullstack-dms.git"
  },
  "keywords": [
    "react",
    "express",
    "node",
    "documents",
    "APIs"
  ],
  "author": "Awa Desmoline",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-aawa/fullstack-dms/issues"
  },
  "homepage": "https://github.com/andela-aawa/fullstack-dms#readme",
  "dependencies": {
    "axios": "0.12.0",
    "babel-register": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-watch": "^2.0.6",
    "bcrypt": "^0.8.7",
    "babel-cli": "^6.24.0",
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "env2": "^2.1.1",
    "enzyme": "^2.8.2",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.2",
    "morgan": "^1.8.1",
    "nightwatch": "^0.9.14",
    "nodemon": "^1.11.0",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.1",
    "react-materialize": "^0.18.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.0",
    "react-tinymce": "^0.5.1",
    "redux": "^3.5.2",
    "redux-form": "^6.6.2",
    "redux-thunk": "^2.1.0",
    "selenium-download": "^2.0.10",
    "sequelize": "^3.30.4",
    "rimraf": "^2.6.1",
    "sequelize-cli": "^2.7.0",
    "shortid": "^2.2.8",
    "swagger-jsdoc": "^1.9.2",
    "toastr": "^2.1.2",
    "validator": "^5.7.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.1",
    "css-loader": "^0.28.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.12.0",
    "material-ui": "^0.17.3",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.5.1",
    "supertest": "^3.0.0"
  }
}
